<!DOCTYPE html>
<html>
<head>
  <title>RemindavaxPilot</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
  <div id="wrap">
    <%= render 'layouts/header' %>
    <section id="flash">
      <% flash.each do |key, value| %>
        <div class="<%= key %>"><%= value %></div>
      <% end %>
    </section>
    <%= yield %>
  </div>
  <%= debug(params) if Rails.env.development? %>
</body>
</html>
