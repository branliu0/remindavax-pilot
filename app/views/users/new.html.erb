<h1>Create User</h1>
<%= semantic_form_for @user do |f| %>
  <%= f.inputs do %>
    <%= f.input :username %>
    <%= f.input :password %>
    <%= f.input :password_confirmation %>
    <%= f.input :phc, :collection => @phcs %>
    <%= f.input :admin %>
  <% end %>
  <%= f.buttons do %>
    <%= f.commit_button %>
  <% end %>
<% end %>
